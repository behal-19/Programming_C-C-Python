#include<stdio.h>
int main()
{
    int x,y,z,p;
    printf("Enter:");
    scanf("%d",&x);
    printf("Enter:");
    scanf("%d",&y);
    printf("Enter:");
    scanf("%d",&z);
    if ((x+y)>z && (y+z)>x && (z+x)>y)
    {
        printf("This is a triangle");
        p=x+y+z;
        printf("\nThe perimeter is %d",p);
    }
    return 0;
}
